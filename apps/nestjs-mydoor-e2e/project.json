{
  "name": "nestjs-mydoor-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["nestjs-mydoor"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/nestjs-mydoor-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": ["nestjs-mydoor:build", "nestjs-mydoor:serve"]
    }
  }
}
